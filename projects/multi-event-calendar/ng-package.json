      "dayjs/plugin/isBetween": "dayjs/plugin/isBetween",
      "dayjs/plugin/weekday": "dayjs/plugin/weekday",
      "dayjs/plugin/isSameOrBefore": "dayjs/plugin/isSameOrBefore",
      "dayjs/plugin/weekOfYear": "dayjs/plugin/weekOfYear"
    }
  }
}
